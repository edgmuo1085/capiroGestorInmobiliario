{
  "name": "capiro-gestor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=production --aot",
    "build:dev": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=development --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:write": "prettier --write \"**/src/**/*.{ts,js,json,html,css,scss,md}\"",
    "format:check": "prettier --check \"**/src/**/*.{ts,js,json,html,css,scss,md}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^5.2.3",
    "image-to-base64": "^2.2.0",
    "prettier": "^2.8.4",
    "primeicons": "^6.0.1",
    "primeng": "^14.0.2",
    "rxjs": "~7.5.0",
    "sweetalert2": "^11.4.8",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "^14.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
